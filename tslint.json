{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {
    "object-literal-sort-keys": false,
    "ordered-imports": [false],
    "no-console": false
  },
  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": [false],
    "jsx-boolean-value": false,
    "member-ordering": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
